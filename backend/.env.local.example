# Supabase
SUPABASE_URL=https://YOUR_PROJECT.supabase.co
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY
SUPABASE_TABLE=hourly_metrics
SUPABASE_ORDERS_TABLE=shopify_orders
SUPABASE_ORDER_LINES_TABLE=shopify_order_lines

# Data sources
APPS_SCRIPT_URL=https://script.google.com/macros/s/REPLACE_ME/exec
SHOPIFY_STORE_DOMAIN=elave.myshopify.com
SHOPIFY_ACCESS_TOKEN=shpat_REPLACE_ME
SHOPIFY_API_VERSION=2024-10
REPORTING_TIMEZONE=UTC
SYNC_DAYS=90
MONTHLY_SALES_TARGET=

# API server
PORT=8787

# Google Calendar OAuth (optional utility cell)
GOOGLE_OAUTH_CLIENT_ID=
GOOGLE_OAUTH_CLIENT_SECRET=
# Optional override. If empty, defaults to http(s)://<api-host>/api/google/oauth/callback
GOOGLE_OAUTH_REDIRECT_URI=
# Optional. Defaults to calendar readonly scope.
GOOGLE_OAUTH_SCOPES=
# Optional. Defaults to "primary"
GOOGLE_CALENDAR_ID=primary
# Optional persistent token. If empty, connect once via /api/google/oauth/start at runtime.
GOOGLE_CALENDAR_REFRESH_TOKEN=
